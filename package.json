{
  "name": "nodeliveme",
  "version": "1.0.0",
  "description": "nodeliveme",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "pm2": "sudo pm2 start ./bin/www --name 'm.liveme.com'",
    "max": "sudo pm2 start ./bin/www -i max --name 'm.liveme.com'",
    "nodemon": "sudo nodemon --exec 'npm run prod'",
    "development": "NODE_ENV=production node ./bin/www",
    "dev": "cross-env PORT=3000 NODE_ENV=development nodemon start",
    "production": "NODE_ENV=production node ./bin/www",
    "prod": "cross-env PORT=3000 NODE_ENV=production nodemon start",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "~1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.1.1",
    "debug": "~2.6.9",
    "ejs": "^2.5.7",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "firebase": "4.6.2",
    "google-protobuf": "^3.5.0",
    "jade": "~1.11.0",
    "lodash": "^4.17.5",
    "lru-cache": "^4.1.1",
    "morgan": "~1.9.0",
    "pm2": "^2.10.1",
    "qs": "^6.5.2",
    "rangeslider-pure": "^0.4.5-1",
    "route-cache": "0.4.3",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.16",
    "vue-awesome-swiper": "^3.1.3",
    "vue-bus": "^1.1.0",
    "vue-i18n": "^7.8.0",
    "vue-infinite-scroll": "^2.0.2",
    "vue-meta": "^1.5.2",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "window": "^4.2.5"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chokidar": "^1.7.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "vue-loader": "^15.2.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  }
}
